  "keywords": [
    "figma",
    "plugin",
    "react",
    "icons",
    "export",
    "framer-motion"
  ],
  "author": {
    "name": "IvanDF",
    "url": "https://ivandf.netlify.app"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/IvanDF/figma-react-icons-exporter.git"
  },
  "bugs": {
    "url": "https://github.com/IvanDF/figma-react-icons-exporter/issues"
  },
  "homepage": "https://github.com/IvanDF/figma-react-icons-exporter#readme",
  "devDependencies": {
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^8.0.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^29.0.0",
    "prettier": "^3.0.0",
    "typescript": "^5.0.0",
    "webpack": "^5.0.0",
    "webpack-cli": "^5.0.0",
    "webpack-dev-server": "^4.0.0"
