{
  "name": "figma-react-icons-exporter",
  "version": "1.0.0",
  "description": "A Figma plugin to export frames as React components with optional Framer Motion support.",
  "main": "code.ts",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "lint": "eslint --ext .ts,.tsx --ignore-pattern node_modules .",
    "lint:fix": "eslint --ext .ts,.tsx --ignore-pattern node_modules --fix .",
    "watch": "npm run build -- --watch"
